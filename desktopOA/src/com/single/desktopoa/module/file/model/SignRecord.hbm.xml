<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="SignRecord" name="com.single.desktopoa.module.file.model.SignRecord">
    <id access="field" name="id">
      <generator class="native"/>
    </id>
    <many-to-one access="field" name="signer"/>
    <property name="signDate" access="field"/>
    <property name="suggest" access="field"/>
    <many-to-one access="field" name="counterSign"/>
    <property name="signed" access="field"/>
  </class>
</hibernate-mapping>
